{
  "extends": "../../../../../tsconfig",
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["esnext", "dom"],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "layout/*": ["aeroelastic/*"]
    }
  }
}
